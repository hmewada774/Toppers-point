# Copy this file to .env and fill values before running in production
# Environment: production or development
NODE_ENV=production

# Web server
PORT=5666
# Comma-separated list of allowed origins for CORS
CORS_ORIGINS=https://topperspoint.in,http://localhost:5666
# How long to cache static assets if not immutable (e.g., 1d, 12h)
STATIC_MAX_AGE=1d

# Session
SESSION_SECRET=change_this_to_a_long_random_string

# MongoDB
# Example: mongodb+srv://<user>:<pass>@<cluster>/<db>?retryWrites=true&w=majority
MONGO_URI=
